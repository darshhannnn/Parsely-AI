{
  "version": 2,
  "builds": [
    {
      "src": "src/api/hackathon_main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/hackathon_main.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "HACKATHON_API_TOKEN": "@hackathon_api_token",
    "LLM_MODEL": "gemini-1.5-flash"
  }
}